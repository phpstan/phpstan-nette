{
	"name": "phpstan/phpstan-nette",
	"description": "Nette Framework class reflection extension for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.9-dev"
		}
	},
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "^0.9",
		"nette/component-model": "^2.3.0 || ^3.0.0",
		"nette/di": "^2.3.0 || ^3.0.0",
		"nette/forms": "^2.3.0 || ^3.0.0",
		"nette/utils": "^2.3.0 || ^3.0.0"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.13.0",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.13.0",
		"phpstan/phpstan-php-parser": "^0.9",
		"phpstan/phpstan-phpunit": "^0.9",
		"phpstan/phpstan-strict-rules": "^0.9",
		"phpunit/phpunit": "^6.2",
		"satooshi/php-coveralls": "^1.0",
		"slevomat/coding-standard": "^2.0"
	},
	"autoload": {
		"psr-4": {
  			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
